class Cat
    def initialize(name,breed,age)
        @name = name
        @breed = breed
        @age = age
    end

    def meow
        puts("Meow")
    end

    def introduce
        puts("Hi! I'm #{@name} and I'm a #{@age} year old #{@breed}")
    end
end

